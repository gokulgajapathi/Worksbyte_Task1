{
    "version": 2,
    "projects": {
      "frontend": { "src": "/frontend", "buildCommand": "npm run build" },
      "backend": { "src": "/backend/api", "use": "@vercel/node" }
    },
    "routes": [
      { "src": "/api/(.*)", "dest": "/backend/api/$1" },
      { "src": "/(.*)", "dest": "/frontend/$1" }
    ]
  }
  